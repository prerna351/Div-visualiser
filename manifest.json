
{
    "manifest_version": 3,
    "name": "Div Colorizer",
    "version": "0.0.1",
    "description": "Add colors to all div containers on a website to visualize the layout.",
    "permissions": [
      "activeTab",
      "scripting",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html"
    },
   
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  